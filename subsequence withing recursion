
void printF(int ind , vector<int>& ds , vector<int>& nums , int n){
    if (ind == n ){
        for( auto it : ds){
             cout<< it << " ";
        }
        if( ds.size() == 0){
             cout<< " {} " << " ";
        }
        cout<< endl;
        return ;
    }
    printF(ind+1,ds,nums,n);
    ds.push_back(nums[ind]);
    printF(ind+1, ds ,nums,n);
    ds.pop_back();
}

//Complexity : The time complexity is O(n2^n) and Space complexity is O(n).
    
